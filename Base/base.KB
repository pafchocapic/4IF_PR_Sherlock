    <METARULE  name="Règle1 de Base de Méta Règles">
     <Param> $1:²Règle </Param>
     <If>
$1§utilisée=vrai
     </If>
     <Then>
Calculer(C§nomRègleUtilisée=C§nomRègleUtilisée+"\n")
Calculer(C§nomRègleUtilisée=C§nomRègleUtilisée+$1§nom)
     </Then>
    </METARULE>
  </METABASE>
