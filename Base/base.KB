     </Then>
    </RULE>
    <RULE  name="NvellerègleInco">
     <If>
      C§défaut=incohérence non gérée
     </If>
     <Then>
      Saisir(C§nomRègleInco)
      Déposer("regles.reg",nouvelle règle incohérence)
     </Then>
    </RULE>
    <RULE  name="NvellerègleTort">
     <If>
      C§défaut=torts inconnus
     </If>
     <Then>
      Saisir(C§nomRègleTort)
      Saisir(A§NouveauTort)
      Saisir(B§NouveauTort)
      Déposer("regles.reg",nouvelle règle tort)
     </Then>
    </RULE>
