 <TASKS>
  <TASK  name="Tâche Principale">
    <Method>
(Présenter(Ecran Accueil) ,
 Présenter(Ecran Constat) ,
 Libérer(Coherence du constat) ,
 Trouver(Constat§Coherent) ,
 Ecrire(Constat§Coherent) ,
 {C§Coherent}
   (Libérer(Fautes graves) ,
    Trouver(Vehicule§Categorie) ,
    Trouver(Vehicule§Tort) ,
    {inconnu(A§Tort)}
      (Libérer(Cas portiere) ,
       Trouver(Vehicule§Tort) ,
       {inconnu(A§Tort)}
         (Libérer(Vehicule en stationnement) ,
          Trouver(Vehicule§Categorie) ,
          Trouver(Vehicule§Tort) ,
          {inconnu(A§Tort)}
            (Libérer(Vehicules sur chaussees differentes) ,
             Trouver(Vehicule§Categorie) ,
             Trouver(Vehicule§Tort) ,
             {inconnu(A§Tort)}
               (Libérer(Vehicules sur memes chaussees) ,
                Trouver(Vehicule§Categorie) ,
                Trouver(Vehicule§SousCategorie) ,
                Trouver(Vehicule§Tort) ,
                {inconnu(A§Tort)}
                  (Libérer(Vehicule quitant le stationnement) ,
                   Trouver(Vehicule§Categorie) ,
                   Trouver(Vehicule§Tort) ) ) ) ) ) ) ,
 {(C§Choix=Maintenir)}
   (Libérer(Maintenance) ,
    Présenter(Ecran résultat) ,
    {(C§défaut=incohérence non gérée)}
      Présenter(nouvelle règle incohérence) ,
    {(C§défaut=torts erronés)}
      Présenter(modification de règle) ,
    {(C§défaut=torts inconnus)}
      Présenter(nouvelle règle tort) )
|
   Présenter(Ecran résultat simple) )
    </Method>
  </TASK>
  <CONTROLTASK  name="²Traitement Incohérence">
    <Method>
(Message("Nous sommes en présence d'une incohérence. Le traitement est stoppé") ,
 Arrêt )
    </Method>
  </CONTROLTASK>
 </TASKS>
