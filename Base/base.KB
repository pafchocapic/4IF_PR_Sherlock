 <BASES>
  <BASE  name="Base de Règles">
    <Inhibited> no </Inhibited>
  </BASE>
  <BASE  name="Coherence">
    <PartOf> Base de Règles </PartOf>
    <Inhibited> no </Inhibited>
  </BASE>
  <BASE  name="Mise en coherence">
    <PartOf> Coherence </PartOf>
    <Inhibited> no </Inhibited>
    <RULE  name="Reso Incoherence Cases 16">
     <If>
      A§Case16=vrai
      B§Case16=vrai
     </If>
     <Then>
      Message( "Vous avez indiqué que les deux véhicules provenaient de droite. Reprécisez ces deux informations à partir des croquis et observations : " )
      Saisir(A§Case16)
      Saisir(B§Case16)
     </Then>
    </RULE>
    <RULE  name="Reso Incohérence Cases 12 et 13 pour un meme vehicule">
     <Param> $1:Vehicule </Param>
     <If>
      $1§Case12=vrai
      $1§Case13=vrai
     </If>
     <Then>
      Message( "Pouvez vous, à l'aide des observations, préciser si le véhicule $1 vire à droite ou à gauche" )
      Saisir($1§Case12)
      Saisir($1§Case13)
     </Then>
    </RULE>
    <RULE  name="RésoAuto Cases 1 15">
     <Param> $1:Vehicule </Param>
     <If>
      $1§Case1=vrai
      $1§Case15=vrai
     </If>
     <Then>
      Ajouter($1§StationnementRegulier=faux)
     </Then>
    </RULE>
    <RULE  name="Incohérence Cases 14 et 1">
     <Param> $1:Vehicule </Param>
     <If>
      $1§Case14=vrai
      $1§Case1=vrai
     </If>
     <Then>
      Blanchir($1§Case1)
     </Then>
    </RULE>
    <RULE  name="Incohérence Cases 14 et 2">
     <Param> $1:Vehicule </Param>
     <If>
      $1§Case14=vrai
      $1§Case2=vrai
     </If>
     <Then>
      Blanchir($1§Case2)
     </Then>
    </RULE>
    <RULE  name="Incohérence Cases 14 et 3">
     <Param> $1:Vehicule </Param>
     <If>
      $1§Case14=vrai
      $1§Case3=vrai
     </If>
     <Then>
      Blanchir($1§Case3)
     </Then>
    </RULE>
    <RULE  name="Incohérence Cases 1 et 2">
     <Param> $1:Vehicule </Param>
     <If>
      $1§Case2=vrai
      $1§Case1=vrai
     </If>
     <Then>
      Blanchir($1§Case1)
     </Then>
    </RULE>
    <RULE  name="Incohérence Cases 1 et 3">
     <Param> $1:Vehicule </Param>
     <If>
      $1§Case3=vrai
      $1§Case1=vrai
     </If>
     <Then>
      Blanchir($1§Case1)
     </Then>
    </RULE>
  </BASE>
  <BASE  name="Coherence du constat">
    <PartOf> Coherence </PartOf>
    <Inhibited> no </Inhibited>
    <RULE  name="Incoherence Cases 8">
     <Param> $1:Vehicule </Param>
     <Param> $2:Vehicule </Param>
     <Param> $3:Constat </Param>
     <If>
      $1§Case8=vrai
      $2§Case8=vrai
      $1#$2
     </If>
     <Then>
      Ajouter($3§Coherent=faux)
     </Then>
    </RULE>
    <RULE  name="Incoherence Cases 16">
     <If>
      A§Case16=vrai
      B§Case16=vrai
     </If>
     <Then>
      Ajouter(C§Coherent=faux)
     </Then>
    </RULE>
    <RULE  name="Incohérence Cases 12 et 13 pour un meme vehicule">
     <Param> $1:Vehicule </Param>
     <Param> $2:Constat </Param>
     <If>
      $1§Case12=vrai
      $1§Case13=vrai
     </If>
     <Then>
      Ajouter($2§Coherent=faux)
     </Then>
    </RULE>
    <RULE  name="Incoherence si 1 et 1">
     <Param> $1:Vehicule </Param>
     <Param> $2:Vehicule </Param>
     <Param> $3:Constat </Param>
     <If>
      $1§Case1=vrai
      $2§Case1=vrai
      $1#$2
     </If>
     <Then>
      Ajouter($3§Coherent=faux)
     </Then>
    </RULE>
  </BASE>
  <BASE  name="Torts">
    <PartOf> Base de Règles </PartOf>
    <Inhibited> no </Inhibited>
  </BASE>
  <BASE  name="Fautes graves">
    <PartOf> Torts </PartOf>
    <Inhibited> yes </Inhibited>
    <RULE  name="Case 17 Prio ou Feu">
     <Param> $1:Vehicule </Param>
     <If>
      $1§Case17=vrai
     </If>
     <Then>
      Ajouter($1§Categorie=Faute grave)
     </Then>
    </RULE>
    <RULE  name="Faute grave d'un vehicule">
     <Param> $1:Vehicule </Param>
     <Param> $2:Vehicule </Param>
     <If>
      $1§Categorie=Faute grave
      $2§Categorie#Faute grave
      $1#$2
     </If>
     <Then>
      Ajouter($1§Tort=100)
      Ajouter($2§Tort=0)
     </Then>
    </RULE>
    <RULE  name="Faute Grave observee">
     <Param> $1:Vehicule </Param>
     <If>
      $1§ObsFauteGrave=vrai
     </If>
     <Then>
      Ajouter($1§Categorie=Faute grave)
     </Then>
    </RULE>
    <RULE  name="Faute grave de deux vehicules">
     <Param> $1:Vehicule </Param>
     <Param> $2:Vehicule </Param>
     <If>
      $1§Categorie=Faute grave
      $2§Categorie=Faute grave
      $1#$2
     </If>
     <Then>
      Ajouter($1§Tort=50)
      Ajouter($2§Tort=50)
     </Then>
    </RULE>
  </BASE>
  <BASE  name="Vehicule en stationnement">
    <PartOf> Torts </PartOf>
    <Inhibited> yes </Inhibited>
    <RULE  name="En stationnement case 1">
     <Param> $1:Vehicule </Param>
     <If>
      $1§Case1=vrai
     </If>
     <Then>
      Ajouter($1§Categorie=Stationnement)
     </Then>
    </RULE>
    <RULE  name="Veh en stat regulier">
     <Param> $1:Vehicule </Param>
     <Param> $2:Vehicule </Param>
     <If>
      $1§Categorie=Stationnement
      $1§StationnementRegulier=vrai
      $1#$2
     </If>
     <Then>
      Ajouter($1§Tort=0)
      Ajouter($2§Tort=100)
     </Then>
    </RULE>
    <RULE  name="Veh en stat irreg agglo trottoir">
     <Param> $1:Vehicule </Param>
     <Param> $2:Vehicule </Param>
     <Param> $3:Constat </Param>
     <If>
      $1§Categorie=Stationnement
      $1§StationnementRegulier=faux
      $3§Agglo=vrai
      $1§StationnementTrottoir=vrai
      $1#$2
     </If>
     <Then>
      Ajouter($1§Tort=0)
      Ajouter($2§Tort=100)
     </Then>
    </RULE>
    <RULE  name="Veh en stat irreg agglo pas trottoir">
     <Param> $1:Vehicule </Param>
     <Param> $2:Vehicule </Param>
     <Param> $3:Constat </Param>
     <If>
      $1§Categorie=Stationnement
      $1§StationnementRegulier=faux
      $3§Agglo=vrai
      $1§StationnementTrottoir=faux
      $1#$2
     </If>
     <Then>
      Ajouter($1§Tort=25)
      Ajouter($2§Tort=75)
     </Then>
    </RULE>
    <RULE  name="Veh en stat irreg hors agglo">
     <Param> $1:Vehicule </Param>
     <Param> $2:Vehicule </Param>
     <Param> $3:Constat </Param>
     <If>
      $1§Categorie=Stationnement
      $1§StationnementRegulier=faux
      $3§Agglo=faux
      $1#$2
     </If>
     <Then>
      Ajouter($1§Tort=50)
      Ajouter($2§Tort=50)
     </Then>
    </RULE>
  </BASE>
  <BASE  name="Vehicule quitant le stationnement">
    <PartOf> Torts </PartOf>
    <Inhibited> yes </Inhibited>
    <RULE  name="un vehicule quite le stationnement">
     <Param> $1:Vehicule </Param>
     <Param> $2:Vehicule </Param>
     <If>
